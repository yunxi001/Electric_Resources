            "includePath": [
                "${config:idf.espIdfPath}/frameworks/**",
                "${config:idf.espIdfPathWin}/components/**",
                "${workspaceFolder}/**",
                "managed_components",
                "components",
                "main",
                "squareline",
                "spiffs",
                "factory_nvs"
